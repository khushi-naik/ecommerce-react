,["135","136"],"C:\\Khushi\\Practice\\ecommerce-react\\frontend\\src\\screens\\ProductScreen.js",["137","138"],"import React, { useEffect, useState } from 'react';\r\nimport {Link} from 'react-router-dom';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport { detailsProduct } from '../actions/productActions';\r\n\r\nfunction ProductScreen (props) {\r\n    // console.log(prop.match.params.id);\r\n    // const product_details = data.products.find(function(x){\r\n    //     return x.id === prop.match.params.id;\r\n    // })\r\n    const [qty, setQty] = useState(1);\r\n    const productDetails = useSelector((state) => state.productDetails);\r\n    const { product, loading, error } = productDetails;\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect( () => {\r\n        dispatch(detailsProduct(props.match.params.id));\r\n       \r\n      return() => {\r\n          //cleanup\r\n      }; \r\n    }, []);\r\n\r\n    const handleAddToCart = () => {\r\n        props.history.push(\"/cart/\" + props.match.params.id + \"?qty=\" + qty);\r\n    }\r\n\r\n    return <div>\r\n    <div className=\"back-link\">\r\n    <Link to=\"/\">Return to Previous Page</Link>\r\n    </div>\r\n\r\n    { loading ? <div>loading...</div> :\r\n      error ? <div>{ error }</div> :\r\n      (\r\n        <div className=\"details\">\r\n        <div className=\"details-image\">\r\n            <img src={product.image}></img>\r\n        </div>\r\n        <div className=\"details-info\">\r\n            <ul>\r\n                <li>{ product.name }</li>\r\n                <li className=\"detail-brand-label\">{ product.brand }</li>\r\n                <li className=\"detail-grey-label\">Price: <span className=\"detail-red-label\">${ product.price }</span></li>\r\n                <li>{product.rating } Stars( { product.reviews } Reviews)</li>\r\n            </ul>\r\n        </div>\r\n\r\n        <div className=\"details-action\">\r\n            <ul>\r\n                <li><span className=\"detail-grey-label\">Price:</span><span className=\"detail-red-label\"> ${product.price}</span></li>\r\n                <li><span className=\"detail-grey-label\">Status:</span> <span className=\"detail-green-label\">{product.countInStock>0 ? \"Product in Stock\" : \"Unavailable\"}</span></li>\r\n                <li><span className=\"detail-grey-label\">Quantity:</span> <select value= { qty } onChange={ (e) => { setQty(e.target.value) }}>\r\n                    { [...Array(product.countInStock).keys()].map(x =>\r\n                    <option key={ x+1 } value={ x+1 }>{ x+1 }</option>\r\n                    )}\r\n                </select>\r\n                </li>\r\n                <li>\r\n                { product.countInStock>0 && <button onClick={ handleAddToCart }>Add to Cart</button> }                \r\n                </li>\r\n            </ul>\r\n        </div>\r\n    \r\n    </div>\r\n      )}\r\n    \r\n    \r\n    </div>\r\n    \r\n    \r\n}\r\n\r\nexport default ProductScreen;",["139","140"],"C:\\Khushi\\Practice\\ecommerce-react\\frontend\\src\\actions\\productActions.js",[],"C:\\Khushi\\Practice\\ecommerce-react\\frontend\\src\\constants\\productConstants.js",[],"C:\\Khushi\\Practice\\ecommerce-react\\frontend\\src\\screens\\CartScreen.js",["141","142"],"import React, { useEffect } from 'react';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport { addToCart, removeFromCart } from '../actions/cartActions';\r\nimport { Link } from 'react-router-dom';\r\n\r\nfunction CartScreen(props){\r\nconst cart = useSelector(state => state.cart);\r\nconst { cartItems } = cart;\r\nconst productId = props.match.params.id;\r\nconst qty = props.location.search ? Number(props.location.search.split(\"=\")[1]): 1;\r\nconst dispatch = useDispatch();\r\n\r\nconst removeFromCartHandler = (productId) => {\r\n    dispatch(removeFromCart(productId));\r\n}\r\n\r\nconst checkoutHandler = () => {\r\n    props.history.push(\"/signin?redirect=shipping\")\r\n}\r\n\r\nuseEffect(() => {\r\n    if(productId){\r\n        dispatch(addToCart(productId,qty))\r\n    }\r\n},[])\r\n\r\n    return <div className=\"cart\">\r\n    <div className=\"cart-list\">\r\n        <ul className=\"cart-list-container\">\r\n            <li>\r\n                <h2>Shopping Cart</h2>\r\n                <div>Price</div>\r\n            </li>\r\n            {\r\n                cartItems.length === 0 ? \r\n                <div>\r\n                Your Cart is Empty \r\n                </div> :\r\n                cartItems.map(item => \r\n                <li className=\"cart-item\">\r\n                <div className=\"cart-image\">\r\n                    <img src= { item.image }/>\r\n                </div>\r\n                \r\n                    \r\n                    <div className=\"cart-name\">\r\n                        <div>\r\n                            <Link to={ \"/product/\" + item.product }>\r\n                            { item.name }\r\n                            </Link>\r\n                        </div>\r\n                        <div>\r\n                            <select className=\"quantity\" value={ item.qty } onChange={(e) => dispatch(addToCart(item.product, e.target.value))}>\r\n                            { [...Array(item.countInStock).keys()].map(x =>\r\n                                <option key={ x+1 } value={ x+1 }>Qty: { x+1 }</option>\r\n                            )}                                \r\n                            </select>\r\n                            <button onClick={ () => removeFromCartHandler(item.product) }> Delete</button>\r\n                        </div>\r\n                    </div>\r\n                    <div className=\"cart-price\">\r\n                       ${ item.price }\r\n                    </div>\r\n                \r\n                </li>\r\n                )\r\n            }\r\n        </ul>\r\n    </div>\r\n\r\n    <div className=\"cart-action\">\r\n            <h3>\r\n                Subtotal ( { cartItems.reduce((a, c) => a + c.qty, 0) } items)\r\n                :\r\n            $ { cartItems.reduce((a, c) => a + c.price*c.qty, 0) }\r\n            </h3>\r\n\r\n            <button onClick={ checkoutHandler } disabled={ cartItems.length === 0}> Proceed to CheckOut </button>\r\n    </div>\r\n    </div>\r\n}\r\n\r\nexport default CartScreen;","C:\\Khushi\\Practice\\ecommerce-react\\frontend\\src\\reducers\\cartReducers.js",[],"C:\\Khushi\\Practice\\ecommerce-react\\frontend\\src\\constants\\cartConstants.js",[],"C:\\Khushi\\Practice\\ecommerce-react\\frontend\\src\\actions\\cartActions.js",[],"C:\\Khushi\\Practice\\ecommerce-react\\frontend\\src\\screens\\SigninScreen.js",["143"],"import React, {useEffect, useState} from 'react';\r\nimport {useDispatch, useSelector} from 'react-redux';\r\nimport {Link} from 'react-router-dom';\r\nimport { signin } from '../actions/userActions';\r\n\r\n\r\nfunction SigninScreen (props) {\r\n      \r\n    const [email, setEmail] = useState('');\r\n    const [password, setPassword] = useState('');\r\n    const userSignin = useSelector(state => state.userSignin);\r\n    const {loading, userInfo, error} = userSignin;\r\n    const dispatch = useDispatch();\r\n    const redirect = props.location.search ? props.location.search.split(\"=\")[1] : '/';\r\n\r\n    useEffect( () => {\r\n        if(userInfo){\r\n            props.history.push(redirect);\r\n        }\r\n       \r\n      return() => {\r\n          //cleanup\r\n      }; \r\n    }, [userInfo]);\r\n\r\n   const submitHandler = (e) => {\r\n       e.preventDefault();\r\n       dispatch(signin(email, password));\r\n   }\r\n\r\n    return <div className=\"form\">\r\n        <form onSubmit={submitHandler}>\r\n            <ul className=\"form-container\">\r\n                <li>\r\n                    <h3>SIGN IN</h3>\r\n                </li>\r\n                <li>\r\n                    {loading && <div>loading...</div>}\r\n                    {error && <div>{ error }</div>}